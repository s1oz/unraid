<?xml version="1.0"?>
<Container version="2">
  <Name>nut</Name>
  <Repository>shawly/nut</Repository>
  <Registry>https://hub.docker.com/r/shawly/nut</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/shawly/docker-nut#docker-container-for-nut</Support>
  <Project>https://github.com/shawly/docker-nut#docker-container-for-nut</Project>
  <Overview>nut server docker image for serving switch nsp and xci to tinfoil on local network.</Overview>
  <Category>Other:</Category>
  <WebUI>http://[IP]:[PORT:9000]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/A75G/docker-templates/master/templates/nut.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/A75G/docker-templates/master/templates/icons/nut.png</Icon>
  <ExtraParams>-d</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1641543642</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Port" Target="9000" Default="9000" Mode="tcp" Description="Port" Type="Port" Display="always" Required="false" Mask="false">9000</Config>
  <Config Name="Games Path" Target="/nut/titles" Default="" Mode="rw" Description="Games Path" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/game/</Config>
  <Config Name="Username" Target="ID" Default="" Mode="" Description="Username" Type="Variable" Display="always" Required="false" Mask="false">user</Config>
  <Config Name="Password" Target="PASSWORD" Default="" Mode="" Description="Password" Type="Variable" Display="always" Required="false" Mask="false">passwd</Config>
  <Config Name="appdata" Target="/nut/conf" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/nut</Config>
  <Config Name="NSPOUT" Target="/nut/_NSPOUT" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/xxxx/_NSPOUT/</Config>
</Container>
